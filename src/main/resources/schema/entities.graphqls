
type NamedEntity implements Node  @key(fields : "id") {
    id: ID!
    literal: String!
    type: EntityType!
    startPosition: Int!
    endPosition: Int!
    knowledgeBaseId: String # Contains Letter at the beginning
    knowledgeBaseUrl: String
    createdAt: DateTime!
    updatedAt: DateTime!
}

enum EntityType {
    ORG
    SIEMENS
    MISC
    PER
    LOC
}


